transitions_time:
  server:
    t_sa: {{ server_t_sa }}
    t_sc:
      {% for i in range(nb_deps_tot) -%}
      - {{ server_t_sc[i] }}
      {% endfor %}
    t_sr: {{ server_t_sr }}
    t_ss:
      {% for i in range(nb_deps_tot) -%}
      - {{ server_t_ss[i] }}
      {% endfor %}
    t_sp:
      {% for i in range(nb_deps_tot) -%}
      - {{ server_t_sp[i] }}
      {% endfor %}
  {% for i in range(nb_deps_tot) %}
  dep{{ i }}:
    id: {{ i }}
    t_di: {{ deps_t_di[i] }}
    t_dr: {{ deps_t_dr[i] }}
    t_du: {{ deps_t_du[i] }}
  {% endfor %}
